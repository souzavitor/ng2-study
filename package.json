    "karma-webpack": "^1.8.0",
    "node-sass": "^4.4.0",
    "protractor": "^4.0.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.0.0",
    "ts-loader": "^1.2.0",
